{
  "name": "overanalyzed",
  "version": "1.0.0",
  "main": "build/web-server/src/server.js",
  "scripts": {
    "bundle-client-dev": "webpack",
    "bundle-client-prod": "webpack -p",
    "clean": "rm -rf build",
    "compile": "tsc -p .",
    "dev": "npm-run-all clean lint compile bundle-client-dev run-server",
    "lint": "tslint -c tslint.json 'web-server/**/*.ts*' 'client/**/*.ts*'",
    "prod": "npm-run-all clean lint compile bundle-client-prod remove-client-maps run-server",
    "remove-client-maps": "rm build/web-server/client-bundle/*.map",
    "run-server": "node build/web-server/src/server.js",
    "test": "npm-run-all clean lint compile test-server",
    "test-server": "mocha build/web-server/test"
  },
  "repository": {
    "url": "git@github.com:briann/template.git",
    "type": "git"
  },
  "author": "Brian Ngo <brian@brianngo.net>",
  "license": "UNLICENSED",
  "devDependencies": {
    "@types/chai": "^3.4.34",
    "@types/express": "^4.0.34",
    "@types/lodash": "^4.14.43",
    "@types/mocha": "^2.2.32",
    "chai": "^3.5.0",
    "mocha": "^3.1.2",
    "npm-run-all": "^3.1.1",
    "source-map-loader": "^0.1.5",
    "ts-loader": "^1.3.2",
    "tslint": "^3.15.1",
    "typescript": "^2.1.0",
    "typings": "^2.0.0",
    "webpack": "^1.14.0"
  },
  "dependencies": {
    "compression": "^1.6.2",
    "express": "^4.14.0",
    "lodash": "^4.16.4",
    "pug": "^2.0.0-beta6"
  }
