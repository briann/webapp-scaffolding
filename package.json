{
  "name": "scaffolding",
  "version": "1.0.0",
  "main": "build/web-server/src/server.js",
  "scripts": {
    "clean": "rm -rf build",
    "build:client": "webpack -p",
    "build:server": "tsc -p .",
    "lint": "tslint -c tslint.json 'web-server/**/*.ts*' 'client/**/*.ts*'",
    "server:prod": "NODE_CONFIG_DIR=web-server/config node build/web-server/src/server.js",
    "server:dev": "NODE_CONFIG_DIR=web-server/config nodemon --watch build/web-server build/web-server/src/server.js",
    "watch": "run-p watch:*",
    "watch:client": "webpack -w",
    "watch:server": "tsc -p . -w"
  },
  "repository": {
    "url": "git@github.com:briann/webapp-scaffolding.git",
    "type": "git"
  },
  "author": "Brian Ngo <brian@brianngo.net>",
  "license": "UNLICENSED",
  "devDependencies": {
    "@types/chai": "3.4.34",
    "@types/config": "^0.0.32",
    "@types/dom4": "1.5.20",
    "@types/es6-shim": "0.31.32",
    "@types/express": "^4.0.35",
    "@types/lodash": "^4.14.55",
    "@types/mocha": "^2.2.40",
    "@types/moment": "2.13.0",
    "@types/pure-render-decorator": "0.2.27",
    "@types/react": "^15.0.16",
    "@types/react-addons-css-transition-group": "^15.0.1",
    "@types/react-dom": "^0.14.23",
    "awesome-typescript-loader": "^3.1.2",
    "chai": "3.5.0",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "source-map-loader": "^0.2.0",
    "tslint": "^4.5.1",
    "typescript": "^2.2.1",
    "typings": "^2.1.0",
    "webpack": "^2.2.1"
  },
  "dependencies": {
    "@blueprintjs/core": "1.11.1",
    "@blueprintjs/datetime": "1.9.1",
    "@blueprintjs/table": "1.8.1",
    "compression": "1.6.2",
    "config": "^1.25.1",
    "express": "^4.15.2",
    "js-yaml": "^3.8.2",
    "lodash": "^4.17.4",
    "moment": "2.17.1",
    "pug": "2.0.0-beta6",
    "react": "^15.4.2",
    "react-addons-css-transition-group": "^15.4.2",
    "react-dom": "^15.4.2",
    "reflect-metadata": "^0.1.10"
  }
}
